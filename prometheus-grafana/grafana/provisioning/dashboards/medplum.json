{
  "dashboard": {
    "id": null,
    "title": "CPU and Memory Usage Dashboard",
    "panels": [
      {
        "type": "graph",
        "title": "CPU Usage",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage ({{instance}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "min": 0,
            "max": 100
          },
          {
            "format": "short",
            "min": null,
            "max": null
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "type": "graph",
        "title": "Memory Usage",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "(node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / node_memory_MemTotal_bytes * 100",
            "legendFormat": "Memory Usage ({{instance}})",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "min": 0,
            "max": 100
          },
          {
            "format": "short",
            "min": null,
            "max": null
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      }
    ],
